import java.util.*;
/**
 * @author Javier Octavio
 */
public class Principal {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner input = new Scanner(System.in);
        System.out.println("Materias");
        boolean bMaterias;
        bMaterias = input.nextBoolean();
        if (bMaterias == true) {
            System.out.println("Servicio");
            boolean bServicio;
            bServicio = input.nextBoolean();
            if (bServicio == true) {
                System.out.println("Residencias");
                boolean bResidencias;
                bResidencias = input.nextBoolean();
                if (bResidencias == true) {
                    System.out.println("Te puedes Titular");
                } else {
                    System.out.println("No te puedes Titular");
                }
            } else {
                System.out.println("No te puedes Titular");
            }
        } else {
            System.out.println("No te puedes Titular");
        }
    }
}
