print("¿Cual es tu Edad?")
iEdad = int(input())
while iEdad > 90:
    print("No te pases, Di tu Edad Real")
    iEdad = int(input())
print("¿Cuentas con INE?")
bINE = input()
if iEdad >= 18:
    if bINE == "true":
        print("Puedes Entrar")
    else:
        print("La INE es Necesaria, No Puedes Entrar")
else:
    print("Eres Menor, No Puedes Entrar")
